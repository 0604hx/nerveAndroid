<resources>
    <string name="dialog_ok">确定</string>
    <string name="dialog_cannel">取消</string>
    <string name="dialig_user_input">手动输入&#8230;</string>
    
    
    <!-- 
    	=======================================================
    	Begin 样式
    	=======================================================
     -->
    <style name="backup_selector">
        <item name="android:background">@drawable/shape_dialog_button_ok</item>
        <item name="android:padding">6dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    
    <style name="backup_selector_white" parent="@style/backup_selector">
        <item name="android:background">@drawable/shape_dialog_button_cannel</item>
    </style>
    
    
     <!-- 
    圆角表格用到的格式与大小值
    统一在这里定义，方便转换使用
     -->
    <dimen name="nerve_corner_arrow_size">17px</dimen>
    <color name="nerve_corner_list_press">#006ac1</color>
    <style name="nerve_corner_list_top">
        <item name="android:background">@drawable/nerve_corner_list_top</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="nerve_corner_list_middle" parent="@style/nerve_corner_list_top">
        <item name="android:background">@drawable/nerve_corner_list_middle</item>
    </style>
    <style name="nerve_corner_list_bottom" parent="@style/nerve_corner_list_top">
        <item name="android:background">@drawable/nerve_corner_list_bottom</item>
    </style>
    
    
    <!-- 表格中标题文字 -->
    <style name="nerver_list_title">
        <item name="android:textColor">#3D4550</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!-- 表格中内容文字 -->
    <style name="nerver_list_info" parent="@style/nerver_list_title">
        <item name="android:textColor">#73A7D8</item>
    </style>
    
    <style name="nerve_arrow">
        <item name="android:src">@drawable/arrow</item>
        <item name="android:width">17px</item>
        <item name="android:height">17px</item>
    </style>
    <!-- 
    	=======================================================
    	end 样式
    	=======================================================
     -->
</resources>
